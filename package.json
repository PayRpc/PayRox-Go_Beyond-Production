    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.5.0",
    "typechain": "^8.3.2",
    "typescript": "5.5.4",
    "web3": "^1.10.4"
